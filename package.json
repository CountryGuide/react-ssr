{
	"name": "react-ssr",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"build": "webpack --ignore client",
		"watch": "webpack --ignore client -w",
		"prestart": "npm run build",
		"start": "nodemon ./dist/server.js",
		"client": "webpack client --config webpack.client.config.js",
		"watch:client": "webpack client --config webpack.client.config.js -w",
		"dev": "run-p start watch watch:client"
	},
	"dependencies": {
		"express": "~4.16.0",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"react-redux": "^7.0.3",
		"redux": "^4.0.1",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.4",
		"@babel/core": "^7.4.5",
		"@babel/preset-env": "^7.4.5",
		"@babel/preset-react": "^7.0.0",
		"axios": "^0.19.0",
		"babel-loader": "^8.0.6",
		"nodemon": "^1.19.1",
		"npm-run-all": "^4.1.5",
		"webpack": "^4.33.0",
		"webpack-cli": "^3.3.2",
		"webpack-node-externals": "^1.7.2"
	}
}
